{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "restore",
            "command": "dotnet",
            "type": "process",
            "args": [
                "restore",
                "-r",
                "win-x64"
            ],
            "problemMatcher": "$msCompile",
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-r",
                "win-x64",
                "${workspaceFolder}/SpellCheckerDemo/SpellCheckerDemo.csproj",
                "/property:GenerateFullPaths=true",
                "/nologo",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-r",
                "win-x64",
                "${workspaceFolder}/SpellCheckerDemo/SpellCheckerDemo.csproj",
                "/property:GenerateFullPaths=true",
                "/nologo",
                "/consoleloggerparameters:NoSummary",
                "/property:Configuration=Release"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "-r",
                "win-x64",
                "-c",
                "Release",
                "--self-contained",
                "true",
                "${workspaceFolder}/SpellCheckerDemo/SpellCheckerDemo.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/SpellCheckerDemo/SpellCheckerDemo.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}